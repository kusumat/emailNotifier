<ivysettings>
    <property name="env.EXECUTOR_NUMBER" value="0" override="false"/>
    <caches lockStrategy="artifact-lock" resolutionCacheDir="${ivy.home}/resol-cache${env.EXECUTOR_NUMBER}" />
    <resolvers>
        <chain name="my-resolver-chain" returnFirst="true">
            <filesystem name="local">
                <artifact pattern="${ivy.settings.dir}/lib/[artifact]-[revision].[ext]"/>
            </filesystem>
            <url name="S3">
                <artifact pattern="https://s3-eu-west-1.amazonaws.com/konyappfactorydev-ci0001-storage1/visualizer/plugins/[artifact]_[revision].[ext]" />
            </url>
            <url name="WWW">
                <artifact pattern="http://download.[CLOUD_DOMAIN]/studio/70/plugins/[artifact]_[revision].[ext]" />
            </url>
            <url name="V8">
                <artifact pattern="http://download.[CLOUD_DOMAIN]/visualizer_enterprise/80/plugins/[artifact]_[revision].[ext]"></artifact>
            </url>
        </chain>
    </resolvers>
    <settings defaultResolver="my-resolver-chain"/>
</ivysettings>
