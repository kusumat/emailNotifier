<ivysettings>
    <property name="env.EXECUTOR_NUMBER" value="0" override="false"/>
    <caches lockStrategy="artifact-lock" resolutionCacheDir="${ivy.home}/resol-cache${env.EXECUTOR_NUMBER}" />
    <resolvers>
        <chain name="my-resolver-chain" returnFirst="true">
            <filesystem name="local">
                <artifact pattern="${ivy.settings.dir}/lib/[artifact]-[revision].[ext]"/>
            </filesystem>
            <url name="V9Prod">
                <artifact pattern="https://download.kony.com/visualizer_enterprise/90/plugins/[artifact]_[revision].[ext]"></artifact>
            </url>
            <url name="V9">
                <artifact pattern="https://download.[CLOUD_DOMAIN]/visualizer_enterprise/90/plugins/[artifact]_[revision].[ext]"></artifact>
            </url>
            <url name="V8Prod">
                <artifact pattern="https://download.kony.com/visualizer_enterprise/80/plugins/[artifact]_[revision].[ext]"></artifact>
            </url>
            <url name="V8">
                <artifact pattern="https://download.[CLOUD_DOMAIN]/visualizer_enterprise/80/plugins/[artifact]_[revision].[ext]"></artifact>
            </url>
            <url name="V7">
                <artifact pattern="https://download.[CLOUD_DOMAIN]/studio/70/plugins/[artifact]_[revision].[ext]" />
            </url>
        </chain>
    </resolvers>
    <settings defaultResolver="my-resolver-chain"/>
</ivysettings>
